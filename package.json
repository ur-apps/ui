{
  "name": "ur-ui",
  "version": "0.0.2-1",
  "description": "UI kit for React apps",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "scripts": {
    "start:storybook": "storybook dev -p 6006",
    "build:lib": "rollup -c && tsc-alias -p tsconfig.app.json",
    "build:pack": "npm run build:lib && npm pack",
    "build:storybook": "storybook build",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:prerelease": "npm version prerelease && git push --follow-tags"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./utils": "./lib/utils/index.js",
    "./icons": "./lib/icons/index.js",
    "./styles": "./lib/index.css",
    "./fonts/*": "./lib/fonts/*",
    "./icons/*": "./lib/icons/*",
    "./images/*": "./lib/images/*"
  },
  "typesVersions": {
    "*": {
      "icons": [
        "./lib/icons/index.d.ts"
      ],
      "utils": [
        "./lib/utils/index.d.ts"
      ]
    }
  },
  "pre-commit": [
    "lint:fix"
  ],
  "keywords": [
    "ui-kit",
    "ur apps"
  ],
  "license": "ISC",
  "author": "Garry Freeman <garryfreeman09@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ur-apps/ui.git"
  },
  "homepage": "https://github.com/ur-apps/ui#readme",
  "bugs": {
    "url": "https://github.com/ur-apps/ui/issues"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@svgr/rollup": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@ur-apps/common-fe": "^0.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.20.0",
    "eslint-plugin-storybook": "^0.11.2",
    "jest": "^29.7.0",
    "postcss-url": "^10.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^3.5.0",
    "rollup": "^4.34.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.84.0",
    "sass-embedded": "^1.83.4",
    "storybook": "^8.5.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3",
    "url-loader": "^4.1.1",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "@types/react": "19.x",
    "@types/react-dom": "19.x",
    "react": "19.x",
    "react-dom": "19.x",
    "react-router-dom": "7.x"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "overrides": {
    "@rollup/plugin-eslint": {
      "eslint": "$eslint"
    },
    "@storybook/addon-docs": {
      "react": "$react"
    }
  }
}
