{
  "name": "ur-ui",
  "version": "0.0.1-18",
  "description": "ur ui kit",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "scripts": {
    "build": "webpack && tsc-alias",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:prerelease": "npm version prerelease && git push --follow-tags"
  },
  "files": [
    "lib",
    "storybook-static"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./icons": "./lib/icons.js",
    "./styles": "./lib/styles/index.css",
    "./styles/colors": "./lib/styles/colors.scss",
    "./styles/constants": "./lib/styles/constants.scss",
    "./styles/mixins": "./lib/styles/mixins.scss",
    "./styles/text": "./lib/styles/text.scss",
    "./fonts/": "./lib/fonts/"
  },
  "typesVersions": {
    "*": {
      "icons": [
        "./lib/icons/index.d.ts"
      ]
    }
  },
  "pre-commit": [
    "lint:fix"
  ],
  "keywords": [
    "ui-kit",
    "ur apps"
  ],
  "license": "ISC",
  "author": "Garry Freeman <garryfreeman09@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ur-apps/ui.git"
  },
  "homepage": "https://github.com/ur-apps/ui#readme",
  "bugs": {
    "url": "https://github.com/ur-apps/ui/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@svgr/webpack": "^6.2.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "mini-css-extract-plugin": "^2.5.3",
    "pre-commit": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.2.6",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  }
}
